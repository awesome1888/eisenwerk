{
  "name": "eisnwerk",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^8.6.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.0",
    "docker-build-tool": "^1.0.1",
    "eslint": "^3.16.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.10.0",
    "fs-copy-file-sync": "^1.1.1",
    "fs-equal": "^1.1.1",
    "hard-source-webpack-plugin": "^0.9.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-object-hash": "^1.4.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "read-yaml": "^1.1.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-md5-hash": "0.0.6",
    "webpack2-externals-plugin": "^1.0.0"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.jsx": "eslint"
  },
  "eslintConfig": {
    "globals": {
      "_": true,
      "window": true,
      "Random": true,
      "$": true,
      "analytics": true,
      "document": true,
      "describe": true,
      "it": true,
      "confirm": true,
      "prompt": true,
      "Package": true,
      "atob": true,
      "btoa": true,
      "Image": true,
      "URL": true,
      "ga": true,
      "location": true,
      "google": true,
      "fbq": true,
      "sessionStorage": true,
      "mern": true,
      "t": true,
      "l": true,
      "ObjectId": true,
      "TObjectId": true,
      "Schema": true,
      "navigator": true,
      "mix": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [],
    "extends": [
      "airbnb"
    ],
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "settings": {},
    "rules": {
      "react/forbid-prop-types": [
        0
      ],
      "import/no-absolute-path": [
        0
      ],
      "comma-dangle": [
        0
      ],
      "import/extensions": [
        0
      ],
      "import/no-extraneous-dependencies": [
        0
      ],
      "object-curly-spacing": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "react/jsx-no-bind": [
        0
      ],
      "import/first": [
        0
      ],
      "react/jsx-indent": [
        "error",
        4
      ],
      "react/jsx-indent-props": [
        "error",
        4
      ],
      "react/prop-types": [
        0
      ],
      "indent": [
        "error",
        4
      ],
      "no-param-reassign": [
        0
      ],
      "class-methods-use-this": [
        0
      ],
      "no-else-return": [
        0
      ],
      "no-plusplus": [
        0
      ],
      "react/prefer-stateless-function": [
        0
      ],
      "no-lonely-if": [
        0
      ],
      "jsx-a11y/no-static-element-interactions": [
        0
      ],
      "arrow-body-style": [
        0
      ],
      "max-len": [
        0
      ],
      "jsx-a11y/label-has-for": [
        0
      ],
      "react/sort-comp": [
        0
      ],
      "react/no-danger": [
        0
      ],
      "prefer-destructuring": [
        0
      ],
      "object-curly-newline": [
        0
      ],
      "padded-blocks": [
        0
      ],
      "react/jsx-filename-extension": [
        0
      ]
    }
  }
}
